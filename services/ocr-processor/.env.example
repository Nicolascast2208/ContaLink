# Firebase Admin SDK
# Descarga desde: Firebase Console > Project Settings > Service accounts
FIREBASE_SERVICE_ACCOUNT_PATH=./keys/firebase_admin.json
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com

# Google Cloud Vision API
# Descarga desde: Google Cloud Console > IAM & Admin > Service accounts
GOOGLE_VISION_SERVICE_ACCOUNT_PATH=./keys/google_vision.json
GCP_PROJECT_ID=your_gcp_project_id

# Configuraci√≥n adicional (opcional)
LOG_LEVEL=INFO
MAX_RETRIES=3
SII_CACHE_EXPIRY_DAYS=30
